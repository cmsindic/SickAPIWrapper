# Add to end of ~/.bashrc 
export QT_QPA_PLATFORM=xcb
export PATH="$HOME/.local/bin:$PATH"
alias pprun="poetry run python"
alias sps="chromium-browser 192.168.0.1"
alias connectinternet="ip route del default"
alias ltr="ls -ltr"
alias c="cd ~/Documents/ScanSegmentAPI"
alias s="cd ~/Documents/ScanSegmentAPI && pprun ~/Documents/ScanSegmentAPI/gui.py"
alias ccprep="export QT_QPA_PLATFORM=xcb"
